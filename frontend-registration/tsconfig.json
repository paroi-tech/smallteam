{
  "compilerOptions": {
    "module": "es6",
    "target": "es2017",
    "lib": [
      "dom",
      "es2016",
      "es2017.object"
    ],
    "moduleResolution": "node",
    "sourceMap": true,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strict": true,
  },
  "exclude": [
    "../node_modules"
  ]
}
