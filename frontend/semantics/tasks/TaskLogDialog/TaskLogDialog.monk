<dialog class="TaskLogDialog">
  <header class="TaskLogDialog-header">
    <button class="TaskLogDialog-closeButton js-close" title="Close">
      <span class="fa fa-1x fa-times"></span>
    </button>
  </header>

  <table class="TaskLogDialog-table js-table">
    <caption>List of logs</caption>
      <thead>
        <tr>
          <td>ID</td>
          <td>Date</td>
          <td>Step</td>
          <td>Contributor</td>
        </tr>
      </thead>
      <tbody></tbody>
  </table>
</dialog>
