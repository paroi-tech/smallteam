{
  "compilerOptions": {
    "module": "es6",
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "es2015.core",
      "es2015.iterable",
      "es2015.collection",
      "es2015.promise",
      "es2015.symbol"
